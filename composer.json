{
  "name": "adetolaaremu/binkpay-router",
  "description": "Smart routing package",
  "type": "blinkpay-router",
  "require": {
    "illuminate/support": "^10.0",
    "illuminate/contracts": "^10.0",
    "monolog/monolog": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AdetolaAremu\\BlinkPayRouter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AdetolaAremu\\BlinkPayRouter\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AdetolaAremu\\BlinkPayRouter\\Providers\\PaymentServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "AdetolaAremu",
      "email": "aremutola@gmail.com"
    }
  ],
  "minimum-stability": "stable"
}
